{
  "manifest_version": 3,
  "name": "UNC Libproxy Helper",
  "description": "A simple extension to build a UNC libproxy URL based on the URL of the current page.",
  "version": "0.1.0",
  "icons": {
    "16": "images/unc-16.png",
    "32": "images/unc-32.png",
    "48": "images/unc-64.png",
    "128": "images/unc-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": [
    "activeTab"
  ]
}